      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="assets\sav,wav.wav" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="assets\bg_abstract1.jpeg" />
    <Resource Include="assets\bg_abstract2.jpeg" />
    <Resource Include="assets\bg_ellipses.jpeg" />
    <Resource Include="assets\bg_gradient1.jpeg" />
